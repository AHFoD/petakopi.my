<%= simple_form_for(@coffee_shop) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :district, collection: [] %>
  <%= f.input :state, collection: [] %>

  <%= f.input :google_map %>
  <%= f.input :waze %>
  <%= f.input :instagram %>
  <%= f.input :facebook %>
  <%= f.input :twitter %>

  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
